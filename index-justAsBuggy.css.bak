margin : 0.5rem;
border-radius : 1rem;
line-height : 2rem;
font-size : 1.5rem;
font-weight : 555;
outline : 0;
border : #012880 solid 1px;
background-image : linear-gradient(-180deg, #00FF00 0%, #00AA00 100%);
box-shadow : 1rem 1rem 1.25rem 0 rgb(0, 0, 0, 0.50), 0 -0.25rem 1.5rem rgb(0, 80, 0, 1) inset, 0 0.75rem 0.5rem rgb(255, 255, 255, 0.4) inset, 0 0.25rem 0.5rem 0 rgb(0, 80, 0, 1) inset;
}
.button img {
height : 1.5rem;
}
.button span {
color : transparent;
background-image : linear-gradient(0deg, rgb(0, 0, 77, 1) 25%, navy 100%);
background-clip : text;
filter : drop-shadow(0 2px 2px hsl(290 100% 20% / 1));
}
.button::after {
content : "";
display : block;
height : 0.25rem;
position : absolute;
bottom : 0.75rem;
left : 50%;
transform : translateX(-50%);
width : calc(100% - 2rem);
background : #fff;
border-radius : 100%;
filter : blur(1px);
opacity : 0.5;
background-image : linear-gradient(-270deg, rgb(255, 255, 255, 0.00) 0%, #FFFFFF 20%, #FFFFFF 80%, rgb(255, 255, 255, 0.00) 100%);
}
.button:hover::after {
animation : anim_buttonWhiteLines 1s infinite;
}
@keyframes anim_buttonWhiteLines {
0% {
opacity : 0.35;
filter : blur(2px);
}
50% {
opacity : 0.7;
filter : blur(0);
}
100% {
opacity : 0.35;
filter : blur(2px);
}
}
