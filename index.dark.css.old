}

.vividButton[theme="dark"] {
    border: rgba(255, 255, 255, 0.7);
    box-shadow: rgba(255, 255, 255, 0.4) 2px 2px 4px 4px inset, rgba(0, 0, 0, 0.55) 2px 2px 1px 1px;
